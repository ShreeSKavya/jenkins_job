pipeline {
    agent any
    parameters {
            choice choices: ['one', 'two', 'three'], description: 'pick something', name: 'CHIOCE'
    }
    stages {
        stage('STAGE 1') {
            steps {
                sh 'sleep 5'
            }
            
        }

        stage('STAGE 2') {
            steps {
                sh 'sleep 4'
            }
        }

        stage('STAGE 3') {
            steps {
                sh 'sleep 4'
            }
        }
    }
}